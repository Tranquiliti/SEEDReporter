{
	# Settings for the planet filters.
	# Star systems tagged as "theme_core", "theme_hidden", or "system_abyssal"
	# will be skipped regardless of the filter settings.
	"planetFilters": [
		{
			# Name of the filter, which will be displayed on the report.
			"filterName": "Example filter",

			# If true, the filter will be activated when running the report.
			"isEnabled": false,

			# If specified, only planets within the specified LY from the Core World center will show up
			"distanceFromCore": 25,

			# If true, only planets in systems containing a Gate will show up.
			"inGateSystem": false,

			# If true, only planets within 10 LY of a Domain-era Cryosleeper will show up.
			# More specifically, they must be near a system tagged with "theme_derelict_cryosleeper".
			"inDomainCryosleeperRange": false,

			# If true, only planets within 10 LY of a Coronal Hypershunt will show up.
			# More specifically, they must be near a system tagged with "has_coronal_tap".
			"inCoronalHypershuntRange": false,

			# If specified, only planets in systems with at least the specified number of stable locations will show up
			# Includes both empty stable locations and system objectives like Comm Relays, Sensor Arrays, or Nav Buoys
			"numStableLocations": 3,

			# Only these planet types will show up in the filter.
			# Defining this can speed up the search by avoiding the need to manually search
			# all conditions in planets that can never host the desired conditions.
			"matchesPlanetTypes": [
				"gas_giant",
				"ice_giant",
				"lava",
				"lava_minor",
				"frozen",
				"frozen1",
				"frozen2",
				"frozen3",
				"cryovolcanic",
				"barren",
				"barren_castiron",
				"barren2",
				"barren3",
				"barren_venuslike",
				"rocky_metallic",
				"rocky_unstable",
				"rocky_ice",
				"barren-bombarded",
				"irradiated",
				"toxic",
				"toxic_cold",
				"terran",
				"terran-eccentric",
				"jungle",
				"water",
				"arid",
				"tundra",
				"desert",
				"desert1",
				"barren-desert",
			],

			# The planet must have a specified number or all of these conditions
			# to show up in the SEED report.
			"matchesConditions": [
				"habitable",
				"cold",
				"very_cold",
				"hot",
				"very_hot",
				"tectonic_activity",
				"extreme_tectonic_activity",
				"no_atmosphere",
				"thin_atmosphere",
				"toxic_atmosphere",
				"dense_atmosphere",
				"mild_climate",
				"extreme_weather",
				"low_gravity",
				"high_gravity",
				"irradiated",
				"inimical_biosphere",
				"water_surface",
				"poor_light",
				"dark",
				"meteor_impacts",
				"pollution",
				"ore_sparse",
				"ore_moderate",
				"ore_abundant",
				"ore_rich",
				"ore_ultrarich",
				"rare_ore_sparse",
				"rare_ore_moderate",
				"rare_ore_abundant",
				"rare_ore_rich",
				"rare_ore_ultrarich",
				"volatiles_trace",
				"volatiles_diffuse",
				"volatiles_abundant",
				"volatiles_plentiful",
				"organics_trace",
				"organics_common",
				"organics_abundant",
				"organics_plentiful",
				"farmland_poor",
				"farmland_adequate",
				"farmland_rich",
				"farmland_bountiful",
				"ruins_scattered",
				"ruins_widespread",
				"ruins_extensive",
				"ruins_vast",
				"decivilized",
				"decivilized_subpop",
				"solar_array",
			],

			# Use alongside the "matchesConditions" option!
			# Show only planets that match at least the specified number.
			# of conditions - useful if searching within condition groups, like resource deposits.
			# If omitted, the planet must match ALL conditions in "matchesConditions"
			# to show up in the SEED report.
			"matchesAtLeast": 4,

			# Planets with the conditions specified here will not
			# show up in the report.
			"avoidConditions": [
				"habitable",
				"extreme_tectonic_activity",
				"extreme_weather",
				"rare_ore_sparse",
				"rare_ore_moderate",
				"rare_ore_abundant",
				"rare_ore_rich",
				"rare_ore_ultrarich",
				"volatiles_trace",
				"volatiles_diffuse",
				"volatiles_abundant",
				"volatiles_plentiful",
			],

			# If true, saves the sector seed to file if a planet matching this filter is found
			"saveSeedIfFound": false,
		},
		{
			"filterName": "Orbital Solar Array planets",
			"isEnabled": true,
			"matchesPlanetTypes": [
				# Orbital Solar Arrays only appear on "cat_hab1", "cat_hab2", and "cat_hab3" planets.
				# Thus, Terran planets (which are in "cat_hab4") cannot get the condition.
				"terran-eccentric",
				"jungle",
				"water",
				"arid",
				"tundra",
				"desert",
				"desert1",
				"barren-desert",
			],
			"matchesConditions": [
				"solar_array"
			],
		},
		{
			"filterName": "Autonomous Mining Bore with all 4 resources",
			"isEnabled": true,
			"matchesPlanetTypes": [
				"toxic",
				"barren-desert"
			],
			"matchesConditions": [
				"ore_sparse",
				"ore_moderate",
				"ore_abundant",
				"ore_rich",
				"ore_ultrarich",
				"rare_ore_sparse",
				"rare_ore_moderate",
				"rare_ore_abundant",
				"rare_ore_rich",
				"rare_ore_ultrarich",
				"volatiles_trace",
				"volatiles_diffuse",
				"volatiles_abundant",
				"volatiles_plentiful",
				"organics_trace",
				"organics_common",
				"organics_abundant",
				"organics_plentiful"
			],
			"matchesAtLeast": 4,
			"saveSeedIfFound": true,
		},
		{
			"filterName": "Barren in Gate system (20 LY)",
			"isEnabled": true,
			"distanceFromCore": 20,
			"inGateSystem": true,
			"maxHazardValue": 175, # Only 1 25% hazard rating condition
			"matchesPlanetTypes": [
				"barren",
				"barren_castiron",
				"barren2",
				"barren3",
				"barren_venuslike",
				"rocky_metallic",
				"rocky_unstable",
				"rocky_ice",
				"barren-bombarded"
			]
		},
		{
			"filterName": "Gas Giant with Plentiful Volatiles in Gate system (20 LY)",
			"isEnabled": true,
			"distanceFromCore": 20,
			"inGateSystem": true,
			"maxHazardValue": 175, # Only one 25% hazard rating condition at most
			"matchesPlanetTypes": [
				"gas_giant",
				"ice_giant"
			],
			"matchesConditions": [
				"volatiles_plentiful"
			],
			# Only allow either Hot or Cold (to enhance or neutralize with Orbital Fusion Lamp)
			"avoidConditions": [
				"extreme_weather",
				"poor_light"
			],
			"saveSeedIfFound": true
		},
		{
			"filterName": "Soil Nanites and Fullerene Spool on non-decivilized world",
			"isEnabled": true,
			"matchesPlanetTypes": [
				"terran",
				"terran-eccentric",
				"jungle",
				"water",
				"arid",
				"tundra",
				"desert",
				"desert1",
			],
			"matchesConditions": [
				"farmland_poor",
				"farmland_adequate",
				"farmland_rich",
				"farmland_bountiful"
			],
			"matchesAtLeast": 1,
			"avoidConditions": [
				"rare_ore_sparse",
				"rare_ore_moderate",
				"rare_ore_abundant",
				"rare_ore_rich",
				"rare_ore_ultrarich",
				"volatiles_trace",
				"volatiles_diffuse",
				"volatiles_abundant",
				"volatiles_plentiful",
				"extreme_tectonic_activity",
				"extreme_weather",
				"decivilized"
			]
		},
		{
			"filterName": "Sakura planet (from the Unknown Skies mod)",
			"isEnabled": false,
			"matchesPlanetTypes": [
				"US_sakura"
			]
		}
	],
	
